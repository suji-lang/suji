import std:println
import std:io

source = || {
    return || {
        println("test")
    }
}

destination = || {
    return || {
        lines = io:stdin::read_lines()
        match lines::length() > 0 {
            true: { "output received" }
            false: { "no output" }
        }
    }
}

out = source() | destination()

println(out)  # output received